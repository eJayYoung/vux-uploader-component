  "main": "dist/uploader.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build:demo": "rm -rf dist/example.js && cross-env NODE_ENV=sample webpack --progress --hide-modules",
    "build:prod": "rm -rf dist/ && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env BABEL_ENV=test NODE_ENV=testing karma start test/karma.conf.js --signle-run",
    "lint": "eslint --ext .js,.vue src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "version": "npm run changelog",
    "prepublishOnly": "npm run build:prod",
    "server": "nodemon  server.js"
  },
  "dependencies": {
    "exif-js": "^2.3.0",
    "photoswipe": "^4.1.3"
  },
  "peerDependencies": {
    "vue": ">=2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
