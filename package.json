    "eslint-plugin-vue": "^5.0.0-beta.3",
    "express": "^4.16.3",
    "file-loader": "^1.1.4",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "less": "^3.8.1",
